# List of chunks from the divided original_list; each contains 125 elements
chunks_lst = [original_lst[i:i + 125] for i in range(0, len(original_lst), 125)]

# Final list
result_lst = []

# Adding all the elements of the chunks_lst's sublists by index and appending the result to result_lst
for i in range(len(chunks_lst[0])):
    # Holds the sum of the elements of the same index:
    temp_sum = 0
    for j in range(len(chunks_lst)):
        temp_sum += chunks_lst[j][i]
        # if index belongs to the last element in the sublist:
        if j == len(chunks_lst) - 1:
            result_lst.append(temp_sum)
            temp_sum = 0

print(result_lst)
