FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include	"include/initialConditions"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $turbulentOmega;

boundaryField
{
    "(outlet|outlet_2)"
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    inlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    oversetPatch
    {
        type            overset;
        value           $internalField;
    }
    "(canal|canal_2|rect_hub|rect_blades|prop_hub|prop_blades|shaft)"
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}


