FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              nu_water*;
    rho             rho_water*;
}

air
{
    transportModel  Newtonian;
    nu              nu_air*;
    rho             rho_air*;
}

sigma           0;

